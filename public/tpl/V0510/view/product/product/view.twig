{% extends "layout/layout" %}
{% import 'layout/_lazyFocusImager' as macroLazyFocusImager %}
{% import 'layout/breadcrumbs' as breadcrumbs %}
{% block meta %}
    {{ headTitle(category.name).setSeparator(' - ').setAutoEscape(false)|raw }}
    {% set metaDescription = product.name()~ ' - '~product.price|number_format(0)~' đ' %}
    <meta name="description" content="{{ metaDescription }}">
    <meta name="keywords" content="{{ product.metaKeywords?:product.name }}">
    <meta property="og:url" content="{{ getCurrentUrl() }}">
    <meta property="og:title" content="{{ product.name }}">
    <meta property="og:description" content="{{ metaDescription }}">
    <meta property="og:image" content="{{ 'https:'~product.imageUri }}">
    <meta property="og:type" content="website">
{% endblock %}
{% block stylesheets %}
    <link rel="stylesheet" href="{{ '/js/jquery/cloudzoom/cloudzoom.css' }}" type="text/css">
{% endblock %}
{% block body %}
    {% set ivt = product.inventory().calcAvailable()%}
    {% if(getParam('preview')) %}
        {% set ivt = 0 %}
    {% endif %}
    {% set priceDiscount = product.priceAfterDiscount %}
    {% set showPrice = product.price %}
    {% set oldPrices = null %}
    {% if(priceDiscount > 0) %}
        {% set showPrice = priceDiscount %}
        {% set oldPrices = product.price %}
    {% elseif(product.oldPrice) %}
        {% set oldPrices = product.oldPrice %}
    {% endif %}
    {% set user = getUser() %}
    {% set flag = 0 %}
    {% set variableAttributes = product.variableAttributes %}
    {% set valuesColor = null  %}
    {% set valuesImage = null  %}
    {% set color = variableAttributes['color'] %}
    {% if color is not null or color is not empty %}
        {% set valuesColor = color.childValues %}
    {% endif %}
    {#    Product Attr Images#}
    {% set psAttrImages =  variableAttributes['psimage']%}
    {% if psAttrImages is not null or psAttrImages is not empty %}
        {% set valuesImage = psAttrImages.childValues %}
    {% endif %}
    {% set videoid = "" %}
    {% if(product.video) %}
        {% set videoid = (product.video)|replace({'https://www.youtube.com/watch?v=':''}) %}
    {% endif %}
    {% set buildBread = buildBreadcrumbs(category) %}
    {% set buildBread = buildBread | merge ([{'label': ''~product.name,'uri': 'javascript:void(0)', 'class' : 'msp'}]) %}
    {{ breadcrumbs.build(buildBread) }}

    <section class="product f-left w-100" itemscope itemtype="https://schema.org/Product">
        <div class="section wrap-padding-15 item_product_detail">
            <div class="container">
                <div class="row">
                    <div class="details-product col-xl-9 col-xs-12 pr-md-0 pl-md-0  pr-xl-3 pl-xl-3">
                        <h1 class="title-product">{{ product.name }}</h1>
                        <div class="row">
                            <div class="product-detail-left product-images col-xs-12 col-sm-6 col-md-6 col-lg-5 d-lg-none">
                                <div class="large-image owlnav_style3 relative">
                                    {% if(product.calcDiscountPercent > 0) %}
                                        <div class="flash product">-{{ product.calcDiscountPercent }}%</div>
                                    {% endif %}
                                    <div class="galeray_large owl-carousel owl-theme owl-loaded owl-drag"
                                         data-nav="true" data-loop="false" data-lg-items="1" data-md-items="1"
                                         data-sm-items="1" data-xs-items="1" data-xxs-items="1">
                                        <div class="owl-stage-outer">
                                            <div class="owl-stage">
                                                {% set fancyboxImage = '' %}
                                                {% set childImgs = product.images %}
                                                {% if(childImgs is not empty) %}
                                                    {% for v in childImgs %}
                                                        {% set fancyboxImage %}
                                                            <a href="{{ v.srcUri }}" rel="fyb_group" class="hide"></a>
                                                        {% endset %}
                                                        <div class="owl-item">
                                                            <div class="item" data-src="{{ v.srcUri }}">
                                                                <a href="javascript:void(0)">
                                                                    <img alt="{{ product.name }}" class="cloudzoom-gallery" src="{{ v.thumbnailUri }}"
                                                                         data-cloudzoom="useZoom: '.cloudzoom', image: '{{ v.srcUri }}', zoomImage: '{{ v.srcUri }}'">
                                                                </a>
                                                            </div>
                                                        </div>
                                                    {% endfor %}
                                                {% else %}
                                                    <div class="owl-item">
                                                        <div class="item" data-src="{{ product.imageUri }}">
                                                            <a href="javascript:void(0)">
                                                                <img alt="{{ product.name }}" class="cloudzoom-gallery" src="{{ v.thumbnailUri }}"
                                                                     data-cloudzoom="useZoom: '.cloudzoom', image: '{{ v.srcUri }}', zoomImage: '{{ v.srcUri }}'">
                                                            </a>
                                                        </div>
                                                    </div>
                                                {% endif %}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="product-detail-left product-images col-xs-12 col-sm-6 col-md-5 col-lg-5 d-none d-lg-block">
                                <div class="col_large_default large-image">
                                    {% if(product.calcDiscountPercent > 0) %}
                                        <span class="sale_label">-{{ product.calcDiscountPercent }}%</span>
                                    {% endif %}
                                    <a href="{{ product.imageUri }}" id="single_image" rel="fyb_group">
                                        <img id="img-01" alt="{{ product.name }}" id="z" width="100%"  src="{{ product.imageUri }}"/>
                                    </a>
                                </div>
                                <div id="gallery_02" class="owl-carousel owl-theme thumbnail-product thumb_product_details not-dqowl owl_grid owl-loaded owl-drag" data-loop="false" data-nav="true" data-lg-items="4" data-md-items="4" data-sm-items="3" data-xs-items="3" data-xxs-items="3">
                                    <div class="owl-stage-outer owl-height">
                                        <div class="owl-stage" id="listImgZoom">
                                            {% set fancyboxImage = '' %}
                                            {% set childImgs = product.images %}
                                            {% if(childImgs is not empty) %}
                                                {% for v in childImgs %}
                                                    {% set fancyboxImage %}
                                                        <a href="{{ v.srcUri }}" rel="fyb_group" class="hide"></a>
                                                    {% endset %}
                                                    <div class="owl-item">
                                                        <div class="item" data-src="{{ v.srcUri }}">
                                                            <img alt="{{ product.name }}" class="cloudzoom-gallery" src="{{ v.thumbnailUri }}"
                                                                 data-cloudzoom="useZoom: '.cloudzoom', image: '{{ v.srcUri }}', zoomImage: '{{ v.srcUri }}'">
                                                        </div>
                                                    </div>
                                                {% endfor %}
                                            {% else %}
                                                <div class="owl-item">
                                                    <div class="item" data-src="{{ product.imageUri }}">
                                                        <img alt="{{ product.name }}" class="cloudzoom-gallery" src="{{ v.thumbnailUri }}"
                                                             data-cloudzoom="useZoom: '.cloudzoom', image: '{{ v.srcUri }}', zoomImage: '{{ v.srcUri }}'">
                                                    </div>
                                                </div>
                                            {% endif %}
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-7 details-pro">
                                <div class="fw w-100 forward" itemprop="offers" itemscope="" itemtype="https://schema.org/Offer">
                                    <div class="group-status d-none">
									<span class="first_status status_2"> <span class="line_tt">|</span> Kho:
										<span class="status_name availabel">
											<link itemprop="availability" href="http://schema.org/InStock">
											Còn hàng
										</span>
									</span>
                                    </div>
                                    <div class="price-box">
									<span class="special-price"><span class="price product-price current-price  ProductPrice">{{ showPrice|number_format(0) }}₫</span>
										<meta itemprop="price" content="{{ showPrice|number_format(0) }}">
										<meta itemprop="priceCurrency" content="VND">
									</span> <!-- Giá Khuyến mại -->
                                        {% if(oldPrices > 0) %}
                                            {% set saving = oldPrices > showPrice ? oldPrices - showPrice : 0 %}
                                            <span class="old-price" itemprop="priceSpecification" itemscope="" itemtype="https://schema.org/priceSpecification">
										<del class="price product-price-old original-price sale">{{ oldPrices|number_format(0) }}₫</del>
                                                        <meta itemprop="price" content="{{ oldPrices|number_format(0) }}">
										<meta itemprop="priceCurrency" content="VND">
                                                        </span>
                                            {% if(saving > 0) %}
                                                <span class="saveMoney PriceSaving"> | Tiết kiệm: {{ saving|number_format(0) }}đ</span>
                                            {% endif %}
                                            <span class="saveMoney"> | SALE: {{ product.calcDiscountPercent }}%</span>
                                        {% endif %}
                                        <!-- Giá gốc -->
                                    </div>
                                    <div class="group_spection">
                                        {% set brand = getBrand(product.brandId) %}
                                        {% if(brand is not empty) %}
                                            <div class="vendor d-none d-xl-block">
                                                Nhà sản xuất: <span class="status_name">{{ brand.name }}</span>
                                            </div>
                                        {% endif %}
                                        <div class="sku_code">
                                            Mã sản phẩm: <span class="status_name sku">{{ product.code }}</span>
                                        </div>
                                    </div>
                                    <img class="image_datngay d-none" src="//bizweb.dktcdn.net/100/004/714/themes/779681/assets/datngaynhe.png?1597286652388" alt="đặt ngay nhé">
                                </div>
                                <div id="mobile_order_change" class="form-product col-sm-12">
                                    <form enctype="multipart/form-data" id="add-to-cart-form" action="/cart/add" method="post" class="form-inline MultiFile-intercepted">
                                        <div class="section ">
                                            <div class="box-variant clearfix ">
                                                <input type="hidden" name="variantId" value="32898792">
                                            </div>
                                        </div>
                                        {% if(couponCode) %}
                                            <div style="padding-top: 10px; font-weight: bold; font-size: 14px;">
                                                NHẬP MÃ :
                                                <span style="color: #f44242;font-size: 20px;">{{ couponCode.code }}</span>
                                                GIẢM NGAY {{ couponCode.Option('couponValue') | number_format(0) }} đ
                                            </div>
                                        {% endif %}
                                        {% set psGifts = product.option('psGifts') %}
                                        {% if(psGifts) %}
                                        <div class="blockShip">
                                            <ul class="px-0">
                                                    <li class="psGifts"><i class="fa fa-gift blink"></i><b>Tặng: </b>
                                                        {% for psGift in psGifts %}
                                                            <a href="{{ psGift.viewLink }}" style="color:#555" target="_blank" rel="nofollow">{{ psGift.name }}</a>, '
                                                        {% endfor %}
                                                    </li>
                                            </ul>
                                        </div>
                                        {% endif %}
                                        <div id="product-select-watch" class="select-swatch">
                                            {% set color = variableAttributes['color'] %}
                                            {% set flag=0 %}
                                            {% if color is not null or color is not empty %}
                                                {% set valuesColor = color.childValues %}
                                                {% set numbC =  (valuesColor | length) %}
                                                {% if(valuesColor is not null and numbC>0) %}
                                                    {% set flag = flag + 1 %}
                                                    <div id="variant-swatch-0" class="swatch color req swatch-product-single clearfix" data-option="option1" data-option-index="0" column="{{ color.column }}">
                                                        <div class="header-title">Màu sắc:</div>
                                                        <div class="select-swap">
                                                            {% for val in valuesColor %}
                                                                {% set arrColor = [] %}
                                                                {% set arrColor = arrColor|merge({ (color.column): val.Id }) %}
                                                                {% set returnImg = {'return':'image'} %}
                                                                {% set img = product.attrValueImage(arrColor) %}
                                                                {% set imgZoom = product.attrValueImage(arrColor,false, returnImg) %}
                                                                {#                                                            {% set img = img?:product.thumbnailUri %}#}
                                                                {% set pIdsAttrStr =  product.attrValueImage(arrColor,true) | json_encode() %}
                                                                {% set pIdsAttrStr = pIdsAttrStr | replace({'[': '', ']': ''}) %}
                                                                <div data-value="{{ val.name }}" class="{{ (numbC==1)?'active':'' }} n-sd swatch-element {{ val.name }}" value="{{ val.id }}"thumbnail-item>
                                                                    <input class="variant-0 input-product" id="swatch-0-{{ val.name }}" type="radio" name="option1" value="{{ val.name }}" data-img-src="{{ img }}" data-pIds="{{ pIdsAttrStr }}">
                                                                    <label class="trang has-thumb" for="swatch-0-trang" style="background: url({{ img }}) top left no-repeat ">
                                                                        <span>{{ val.name }}</span>
                                                                        {#                                                                <img class="crossed-out" src="//theme.hstatic.net/1000187250/1000280706/14/soldout.png?v=1588">#}
                                                                        <img class="crossed-out" src="//theme.hstatic.net/1000187250/1000280706/14/soldout.png?v=1588">
                                                                        <img class="img-check" src="//theme.hstatic.net/1000187250/1000280706/14/select-pro.png?v=1588">
                                                                    </label>
                                                                </div>
                                                            {% endfor %}
                                                        </div>
                                                    </div>
                                                {% endif %}
                                            {% endif %}
                                            {% set size = variableAttributes['size'] %}
                                            {% if size is not null or size is not empty %}
                                                {% set valuesSize = size.childValues %}
                                                {% set numbS =  (valuesSize | length) %}
                                                {% if(valuesSize is not null and numbS>0) %}
                                                    {% set flag = flag + 1 %}
                                                    <div id="variant-swatch-1" class="swatch size req swatch-product-single clearfix" column="{{ size.column }}" data-option="option2" data-option-index="1">
                                                        <div class="header-title">Kích thước:</div>
                                                        <div class="select-swap">
                                                            {% for attrValue in valuesSize %}
                                                                <div data-value="{{ attrValue.name }}" value="{{ attrValue.id }}" class="{{ (numbS==1)?'active':'' }} n-sd swatch-element {{ attrValue.name }}" value="{{ attrValue.id }}">
                                                                    <input class="variant-1 input-product" id="swatch-1-{{ attrValue.name }}" type="radio" name="option2" value="{{ attrValue.name }}" checked="">
                                                                    <label for="swatch-1-{{ attrValue.name }}" class="">
                                                                        {{ attrValue.name }}
                                                                        <img class="crossed-out" src="//theme.hstatic.net/1000187250/1000280706/14/soldout.png?v=1588">
                                                                        <img class="img-check" src="//theme.hstatic.net/1000187250/1000280706/14/select-pro.png?v=1588">
                                                                    </label>
                                                                </div>
                                                            {% endfor %}
                                                        </div>
                                                        <script>

                                                        </script>
                                                    </div>
                                                {% endif %}
                                            {%  endif %}
                                            {% if((flag==0) and (product.childs)) %}
                                                {% set flagchilds= product.childs|length %}
                                                <p style="padding: 5px; text-align: left;">
                                                    <span style="width: 131px; display: inline-block">Chọn sản phẩm</span>
                                                    <select class="input childProducts">
                                                        <option value="1">Chọn sản phẩm</option>
                                                        {% set name_parent =product.name %}
                                                        {% for cp in product.childs %}
                                                            <option value="{{ cp.id }}, {{ cp.price }}"
                                                                    href="{{ cp.imageUri }}"
                                                                    data-src="{{ cp.imageUri }}">
                                                                {{ cp.name|replace({ name_parent :''}) }}
                                                            </option>
                                                        {% endfor %}
                                                    </select>
                                                </p>
                                            {% endif %}
                                            {#                                            <div id="variant-swatch-2" class="swatch req swatch-product-single clearfix" data-option="option3" data-option-index="2">
                                                                                            <div class="header">Vật liệu</div>
                                                                                            <div class="select-swap">
                                                                                                <div data-value="Cotton" class="n-sd swatch-element cotton variant-3">
                                                                                                    <input class="variant-2 input-product" id="swatch-2-cotton" type="radio" name="option3" value="Cotton" checked="">
                                                                                                    <label for="swatch-2-cotton" class="sd">
                                                                                                        Cotton
                                                                                                        <img class="crossed-out" src="//theme.hstatic.net/1000187250/1000280706/14/soldout.png?v=1588">
                                                                                                        <img class="img-check" src="//theme.hstatic.net/1000187250/1000280706/14/select-pro.png?v=1588">
                                                                                                    </label>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>#}
                                        </div>
                                        <input type="hidden" id="psQtt" value="1">
                                        <div class="form-group form_button_details">
                                            <div class="form_product_content ">
                                                <div class="soluong d-block">
                                                    <div class="label_sl margin-bottom-5">Số lượng:</div>
                                                    <div class="custom input_number_product custom-btn-number form-control">
                                                        <button class="btn_num num_1 button button_qty" type="button"><i class="fa fa-minus"></i></button>
                                                        <input type="text" id="qtym" name="quantity" min="1" max="{{ ivt }}" maxlength="4" value="1" class="form-control prd_quantity">
                                                        <button class="btn_num num_2 button button_qty" type="button"><i class="fa fa-plus"></i></button>
                                                    </div>
                                                </div>
                                                <div class="nhangiasi d-none">
                                                    <div class="label_sl margin-bottom-5">Nhận mức giá sỉ:</div>
                                                    <div class="check_giasi">
                                                    </div>
                                                </div>
                                                <div class="button_actions clearfix">
                                                    {% set title = '' %}
                                                    {% set ck = '' %}
                                                    {% set class = '' %}
                                                    {% set dataModal = '' %}
                                                    {% if(ivt > 0) %}
                                                        {% set ck = 1 %}
                                                        {% set dataModal = 'data-toggle="modal" data-target="#myModal"' %}
                                                    {% endif %}
                                                    {% if (ivt<0)%}
                                                    {% set title = translate('Sản phẩm tạm hết hàng') %}
                                                    {% set ck = 0 %}
                                                    {% set class = 'outStock' %}
                                                    {% endif %}
                                                    {% if(ivt<=0) %}
                                                        <button class="exclusive outStock {{ ivt <= 0 ? 'd-block':'' }}">
                                                            <button class="btn btn_base btnAddToCart unsel btn_dis btn-cart exclusive outStock d-block {{ ivt <= 0 ? 'd-block':'' }}" selId="{{ product.id }}" psId="{{ product.id }}">
                                                                <span>Sản phẩm tạm hết hàng</span>
                                                            </button>
                                                            <span class="font-weight-light font-italic">Quý khách vui lòng tham khảo sản phẩm tương tự</span>
                                                        </button>
                                                    {% elseif(flag or flagchilds) %}
                                                        <button id="addtoCart" psId="{{ product.id }}" selId="{{ product.id }}" data-original-title="Vui lòng chọn màu sắc hoặc kích cỡ!"
                                                             type="submit" class="btn btn_base btnAddToCart btn_add_cart btn-cart add_to_cart buyCart pdp-mod-sbutton add {{ class }}"   value="{{ ivt }}"
                                                                ck="0" title="{{ title }}">
                                                            <span class="text_1 d-none d-none d-xl-block">Thêm vào giỏ hàng</span>
                                                            <span class="text_1 d-xl-none">Thêm vào giỏ</span>
                                                        </button>
                                                        <button psId="{{ product.id }}" selId="{{ product.id }}" class="fastbuy btnAddToCart buyCart pdp-mod-sbutton add {{ class }}"
                                                                title="{{ title }}" {{ dataModal }} data-original-title="Vui lòng chọn màu sắc hoặc kích cỡ!"
                                                             id="buynow" type="submit" admicro-data-event="GET_VALUE" admicro-data-auto="1" admicro-data-order="false">
                                                            <p class="">Mua ngay</p>
                                                        </button>
                                                        <button class="hotline_phone fastbuy d-lg-none">
                                                            <a href="tel:1900636546">Gọi ngay</a>
                                                        </button>
                                                    {% else %}
                                                        <button id="addtoCart" psId="{{ product.id }}" selId="{{ product.id }}"
                                                             type="submit" class="btn btn_base btnAddToCart btn_add_cart btn-cart add_to_cart buyCart pdp-mod-sbutton add {{ class }}"   value="{{ ivt }}"
                                                             ck="1" title="{{ title }}">
                                                            <span class="text_1 d-none d-none d-xl-block">Thêm vào giỏ hàng</span>
                                                            <span class="text_1 d-xl-none">Thêm vào giỏ</span>
                                                        </button>
                                                        <button psId="{{ product.id }}" selId="{{ product.id }}" class="fastbuy btnAddToCart buyCart pdp-mod-sbutton add {{ class }}"
                                                              title="{{ title }}" {{ dataModal }}
                                                             id="buynow" type="submit" admicro-data-event="GET_VALUE" admicro-data-auto="1" admicro-data-order="false">
                                                            <p class="">Mua ngay</p>
                                                        </button>
                                                        <button class="hotline_phone fastbuy d-lg-none">
                                                            <a href="tel:1900636546">Gọi ngay</a>
                                                        </button>   {% endif %}
                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                    <div class="section shipaddress">
                                        <div class="listadress_ship margin-bottom-15" style="color:red;">
                                        </div>
                                    </div>
                                    <div class="section product-policy d-none d-xl-block">
                                        <div class="content clearfix">
                                            <div class="item">
                                                <i class="fa fa-truck"></i> Miễn phí vận chuyển
                                            </div>
                                            <div class="item">
                                                <i class="fa  fa-gift"></i> Tích điểm trên hóa đơn
                                            </div>
                                            <div class="item">
                                                <i class="fa fa-repeat"></i> Đổi trả hàng
                                            </div>
                                            <div class="item red">
                                                <i class="fa fa-mobile"></i> Hotline: <a href="tel:1900 636 546">1900 636 546</a>
                                            </div>
                                        </div>
                                    </div>
                                    <label for="tab1">{{ translate('Mô tả sản phẩm') }}</label>
                                    <div id="mobile_order_1" class="short-description clearfix section">

                                            {{ product.description | raw}}
                                    </div>
                                </div>
                            </div>

                            <div class="col-xs-12 col-lg-12 col-sm-12 col-md-12">
                                <div class="preview-wrapper">
                                    <div class="web-main-wrapper">
                                        <div class="web-header">
                                            <div class="web-element-text">
                                                <div class="web-element-content">
                                                    <a href="javascript:void(0);">
                                                        <div class="editable-text" data-bind-menu="notification|text_editing">&nbsp;CÓ THỂ BẠN SẼ THÍCH</div>
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="owl-carousel owl_grid owl-loaded owl-drag related" data-dot="false"
                                             data-nav="false" data-loop="false" data-play="false" data-lg-items="5"
                                             data-md-items="2" data-sm-items="2" data-xs-items="2" data-margin="0">
                                            <div class="web-body-wrapper owl-stage-outer">
                                                <div class="web-body owl-stage">
                                                    {% set proReleated = search({'limit':10, 'categoryId':product.categoryId}) %}
                                                    {% if(proReleated is not empty) %}
                                                        {% for p in proReleated %}
                                                            {% set image = p.thumbnailUri %}
                                                            {% set options = {
                                                                alt: p.name,
                                                                lazy: true,
                                                                class: 'imageChange swatch-'~p.id,
                                                                openDiv: false,
                                                            }%}
                                                            {% set priceDiscount = p.priceAfterDiscount %}
                                                            {% set percent = p.discountPercent %}
                                                            <div class="owl-item web-box-item" data-storeId="{{ p.storeId }}" psId="{{ p.id }}" style="position: relative;">
                                                                <div class="web-element-content web-editable-text">
                                                                    <div class="web-inner-box">
                                                                        <a class="web-product-box" href="{{ p.viewLink }}">
                                                                            <div class="web-image-box">
                                                                                {{ macroLazyFocusImager.LazyFocusImager(image,options) }}
                                                                                {% if(percent > 0) %}
                                                                                    <div class="web-discount-badge web-element-content" data-element-name="Discount Badge" data-override-name="true" data-discount-position="topleft" data-percentage-icon-position="aftertheamount">
                                                                                        <div class="web-discount-percentage">{{ percent }}</div>
                                                                                        <div class="web-percentage-icon">%</div>
                                                                                    </div>
                                                                                {%  endif %}
