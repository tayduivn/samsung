<!DOCTYPE html>
<html lang="vi-VN" data-nhanh.vn-template="{{ templateDir }}">
<head>
    <meta name="SKYPE_TOOLBAR" content="SKYPE_TOOLBAR_PARSER_COMPATIBLE"/>
    <meta charset="utf-8">
    {% set appId = getAppId() %}
    {% if(appId) %}
        <meta name="fb:app_id" content="{{ appId }}">
    {% endif %}
    {% set extraSettingsArray =  extraSettings() %}
    {% if(extraSettingsArray is not empty) %}
        {% if(extraSettingsArray['gta']['generatedTimeAndAuthor'] == 1) %}
            {% set display = 'block' %}
        {% endif %}
    {% else %}
        {% set display = 'none' %}
    {% endif %}
    <link href="http://bizweb.dktcdn.net/100/289/886/themes/629773/assets/favicon.png?1594432541646" rel="shortcut icon"
          type="image/vnd.microsoft.icon">
    {{ headMeta().appendName('viewport', 'width=device-width, initial-scale=1.0').appendHttpEquiv('X-UA-Compatible', 'IE=edge')|raw }}
    {% block meta %}{% endblock %}
    {% set styleSheet = [
        '/css/fontAwesome/font-awesome-4.5.0.min.css',
        '/js/jquery/fancybox-2.1.5/source/jquery.fancybox.css',
        '/css/jqueryUI/jqui.css',
        '/css/bootstrap/bootstrap.4.3.1.min.css',
        '/css/owlCarousel/owl.carousel.min.css',
    ] %}

    <link rel="stylesheet" href="{{ srcLink(styleSheet,1) }}" type="text/css">
    {{ loadScripts({'positionHead':true}) | raw }}

</head>
<body>
{% set script = [
    '/js/jquery/jquery.min.js',
    '/js/lib.js',
    '/js/jquery/jquery.cookie.js',
    '/js/jquery/jquery-ui-1.10.3.custom.min.js',
    '/js/jquery/jquery.carouFredSel-6.2.1-packed.js',
    '/js/jquery/fancybox-2.1.5/source/jquery.fancybox.pack.js',
    '/js/jquery/jquery.number.min.js',
    '/js/jquery/jquery.lazyload.min.js',
    '/js/bootstrap/bootstrap.4.3.1.min.js',
    '/js/bootstrap/boostrap.popper.min.js',
    '/js/bootstrap/bootstrap.bundle.min.js',
    '/js/owlCarousel/owl.carousel.min.js',

] %}

<script type="text/javascript" src="{{ srcLink(script,1) }}"></script>
{% block script %}{% endblock %}

<input type="hidden" id="storeId" value="{{ getStoreId() }}">


{% include 'layout/header2' %}
{% block body %}{% endblock %}
{% block stylesheets %}{% endblock %}


{{ loadScripts() | raw }}
{{ headScript }}
{{ inlineScript }}
{#{{ loadFacebookChat('https://www.facebook.com/Capbalovali/') }}#}
{#{% import '/'~templateLib~'/include/skinsnew'%}#}
{% include 'layout/footer2' %}
</body>
</html>