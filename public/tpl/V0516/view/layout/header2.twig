            $(this).find('.link-bottom').addClass('bodder-b');
        })
        $('.dropdown-header1').mouseleave(function () {
            $(this).find('.dropdown-header-content').css('display', 'none');
            $(this).find('.link-bottom').removeClass('bodder-b');
        })
        $('.dropdown-header2:not(:first-child)').mouseenter(function () {
            $('.dropdown-header2:first-child').find('.dropdown-header-content2').css('display', 'none');
            $('.dropdown-header2:first-child').find('.aaa').removeClass('bodder-b');
            $(this).find('.dropdown-header-content2').css('display', 'block');
            $(this).find('.aaa').addClass('bodder-b');
        })
        $('.dropdown-header2:not(:first-child)').mouseleave(function () {
            $(this).find('.dropdown-header-content2').css('display', 'none');
            $(this).find('.aaa').removeClass('bodder-b');
            $('.dropdown-header2:first-child').find('.dropdown-header-content2').css('display', 'block');
            $('.dropdown-header2:first-child').find('.aaa').addClass('bodder-b');
        })

        $('.country').click(function () {
            if ($('.country i').hasClass('fa-caret-down')) {
                $('.country i').removeClass('fa-caret-down').addClass('fa-caret-up');
                $('.country-box').css('display', 'block');
            } else {
                $('.country i').removeClass('fa-caret-up').addClass('fa-caret-down');
                $('.country-box').css('display', 'none');
            }
        })
        $('.item-4').owlCarousel({
            loop: false,
            margin: 10,
            nav: false,
            responsive: {
                0: {
                    items: 1
                },
                1023: {
                    items: 1
                },
                1200: {
                    items: 2
                },
                1400: {
                    items: 3
                },
                1500: {
                    items: 4
                }
            },
        });
    })

    function openNav() {
        document.getElementById("mySidenav").style.display = "block";
    }

    function closeNav() {
        document.getElementById("mySidenav").style.display = "none";
    }

    $(window).scroll(function () {
        var height = $(window).scrollTop();
        if (height > 130 && $(window).width() >= 1200) {
            $('.search-about').addClass('fixed-scroll');
            $('.header-navbar-center').addClass('logo-fixed');
            $('.header-navbar-right').addClass('sign-fixed');
        } else if (height > 80 && $(window).width() < 1200) {
            $('.header-navbar').addClass('fixed-scroll').css('position','fixed');
        } else {
            $('.search-about').removeClass('fixed-scroll');
            $('.header-navbar-center').removeClass('logo-fixed');
            $('.header-navbar-right').removeClass('sign-fixed');
            $('.header-navbar').removeClass('fixed-scroll').css('position','relative');
        }
    });
</script>